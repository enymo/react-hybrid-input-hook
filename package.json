{
  "name": "@enymo-internal/react-hybrid-input-hook",
  "version": "1.0.10",
  "description": "Hook to create inputs that work both independent and as part of a react form",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.internal.enymo.com/enymo/library/react-hybrid-input-hook.git"
  },
  "author": "enymo GmbH",
  "license": "UNLICENSED",
  "publishConfig": {
    "@enymo-internal:registry": "https://gitlab.internal.enymo.com/api/v4/projects/68/packages/npm/"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-native": "^0.70.4",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-hook-form": "^7.34.2"
  }
}
